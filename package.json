{
  "name": "zheng.works",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy-heroku": "git push heroku main",
    "submit-docker": "gcloud builds submit --tag gcr.io/zheng-works-australia/nextjs .",
    "deploy": "npm run submit-docker && gcloud beta run deploy zhengdotworks --image gcr.io/zheng-works-australia/nextjs --region australia-southeast1 --platform managed --allow-unauthenticated --quiet"
  },
  "engines": {
    "node": "17.x"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "cannon-es": "^0.19.0",
    "dat.gui": "^0.7.9",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "formik": "^2.2.9",
    "gltf-webpack-loader": "^1.0.6",
    "next": "12.1.0",
    "randomstring": "^1.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "three": "^0.138.3",
    "typescript": "^4.6.2",
    "uniqid": "^5.4.0",
    "zustand": "^4.0.0-rc.1"
  },
